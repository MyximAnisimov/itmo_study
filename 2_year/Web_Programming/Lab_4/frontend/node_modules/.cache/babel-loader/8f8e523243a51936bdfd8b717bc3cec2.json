ARA;AASA,KAbA;AAcA,IAAA,QAdA,oBAcA,CAdA,EAcA;AAAA;;AACA,MAAA,CAAA,CAAA,cAAA;AACA,WAAA,MAAA,CAAA,GAAA,CAAA,gCAAA,EAAA;AACA,QAAA,KAAA,EAAA,KAAA,KADA;AAEA,QAAA,QAAA,EAAA,KAAA;AAFA,OAAA,EAGA,IAHA,CAGA,YAAA;AACA,QAAA,MAAA,CAAA,OAAA,CAAA;AACA,UAAA,KAAA,EAAA,SADA;AAEA,UAAA,KAAA,EAAA,aAFA;AAGA,UAAA,IAAA,EAAA,wBAHA;AAIA,UAAA,IAAA,EAAA;AAJA,SAAA;AAMA,OAVA,EAUA,KAVA,CAUA,UAAA,KAAA,EAAA;AACA,QAAA,MAAA,CAAA,iBAAA,CAAA,KAAA,CAAA,QAAA,CAAA,IAAA;AACA,OAZA;AAaA,KA7BA;AA8BA,IAAA,iBA9BA,6BA8BA,SA9BA,EA8BA;AACA,WAAA,OAAA,CAAA;AACA,QAAA,KAAA,EAAA,OADA;AAEA,QAAA,KAAA,EAAA,OAFA;AAGA,QAAA,IAAA,EAAA,SAHA;AAIA,QAAA,IAAA,EAAA;AAJA,OAAA;AAMA;AArCA;AAXA,CAAA","sourcesContent":["<template>\r\n  <div id=\"content\">\r\n    <h1>Вход в систему</h1>\r\n\r\n    <form id=\"form\" @sumbit.prevent=\"logIn\">\r\n      <div id=\"login\">\r\n        <label for=\"loginInput\">Введите логин:</label>\r\n        <input class=\"inputs\"\r\n            type=\"text\"\r\n               id=\"loginInput\"\r\n               required\r\n               placeholder=\"Логин\"\r\n               v-model.trim=\"login\"/>\r\n      </div>\r\n      <div id=\"password\">\r\n        <label for=\"passwordInput\">Введите пароль:</label>\r\n        <input\r\n            class=\"inputs\"\r\n            type=\"password\"\r\n               id=\"passwordInput\"\r\n               required\r\n               placeholder=\"Пароль\"\r\n               v-model.trim=\"password\"/>\r\n      </div>\r\n      <div id=\"buttons\">\r\n        <Button color=\"blue\" label=\"Войти в систему\" @click.native=\"logIn\"/>\r\n        <Button color=\"white\" style=\"color: black\" label=\"Зарегистрироваться\" @click.native=\"register\"/>\r\n      </div>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Button from \"@/components/Button\";\r\n\r\nexport default {\r\n  name: \"Registration\",\r\n  components: {\r\n    Button,\r\n  },\r\n  data(){\r\n    return{\r\n      login: \"\",\r\n      password: \"\"\r\n    }\r\n  },\r\n  methods: {\r\n    logIn(e){\r\n      e.preventDefault()\r\n      this.$router.push({name: 'app-page'});\r\n      this.$axios.post('http://localhost:8080/api/user', {\r\n        login: this.login,\r\n        password: this.password\r\n      }).then(response => {\r\n        localStorage.setItem(\"jwt\", response.data);\r\n        this.$router.push({name: 'app-page'});\r\n      }).catch(error => {\r\n        this.AxiosErrorHandler(error.response.data);\r\n      });\r\n    },\r\n    register(e){\r\n      e.preventDefault();\r\n      this.$axios.put('http://localhost:8080/api/user', {\r\n        login: this.login,\r\n        password: this.password\r\n      }).then(() => {\r\n        this.$notify({\r\n          group: 'success',\r\n          title: 'Регистрация',\r\n          text: 'Вы теперь можете войти',\r\n          type: 'success'\r\n        });\r\n      }).catch(error => {\r\n        this.AxiosErrorHandler(error.response.data);\r\n      });\r\n    },\r\n    AxiosErrorHandler(errorText){\r\n      this.$notify({\r\n        group: 'error',\r\n        title: 'Error',\r\n        text: errorText,\r\n        type: 'error'\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n#form button {\r\n  margin: 20px 10px 10px 10px;\r\n}\r\n#login, #password {\r\n  margin: 5px;\r\n}\r\n\r\n#form {\r\n  position: relative;\r\n  font-size: 20px;\r\n  flex-direction: column;\r\n  margin: 30px auto auto;\r\n  border-radius: 20px;\r\n  background: linear-gradient(to bottom left, rgb(220, 63, 7, 0.8), rgb(130, 30, 145, 0.8));\r\n  padding: 20px;\r\n  display: table;\r\n  text-align: center;\r\n  box-shadow: 0 0 10px 1px black;\r\n}\r\n\r\ninput {\r\n  margin-left: 13px;\r\n  padding: 5px 0 5px 2px;\r\n}\r\n.inputs{\r\n  color: black;\r\n  border-radius: 5px;\r\n  border-color:antiquewhite;\r\n}\r\n\r\n</style>"],"sourceRoot":"src/views"}]}